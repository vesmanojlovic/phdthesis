	langid = {english},
	doi = {10.1101/2021.04.30.442123},
	file = {Majic et al. - 2021 - The adaptive potential of non-heritable somatic mu.pdf:/Users/vesheljinn/Zotero/storage/UREF49RD/Majic et al. - 2021 - The adaptive potential of non-heritable somatic mu.pdf:application/pdf},
}

