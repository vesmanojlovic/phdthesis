	langid = {english},
	keywords = {60G51, 92D10, 92D20, Branching process, Inspection paradox, Mutation rates, Primary 60J80, Reproductive bias, Secondary 60K05, Spines, Uniform sampling},
	file = {Full Text PDF:/Users/vesheljinn/Zotero/storage/AQ464LEP/Cheek and Johnston - 2023 - Ancestral reproductive bias in branching processes.pdf:application/pdf},
}

