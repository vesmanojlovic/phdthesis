	pmid = {3934395},
	keywords = {Humans, Models, Genetic, Animals, Biological Evolution, {DNA}, Mitochondrial, Genes, Haplorhini, Mathematics, Nucleic Acid Hybridization, Primates, Proteins, Species Specificity},
}

