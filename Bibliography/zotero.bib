}

@article{west_normal_2021,
	title = {Normal tissue architecture determines the evolutionary course of cancer},
	volume = {12},
	rights = {2021 The Author(s)},
	issn = {2041-1723},
	url = {https://www.nature.com/articles/s41467-021-22123-1},
	doi = {10.1038/s41467-021-22123-1},
	abstract = {Cancer growth can be described as a caricature of the renewal process of the tissue of origin, where the tissue architecture has a strong influence on the evolutionary dynamics within the tumor. Using a classic, well-studied model of tumor evolution (a passenger-driver mutation model) we systematically alter spatial constraints and cell mixing rates to show how tissue structure influences functional (driver) mutations and genetic heterogeneity over time. This approach explores a key mechanism behind both inter-patient and intratumoral tumor heterogeneity: competition for space. Time-varying competition leads to an emergent transition from Darwinian premalignant growth to subsequent invasive neutral tumor growth. Initial spatial constraints determine the emergent mode of evolution (Darwinian to neutral) without a change in cell-specific mutation rate or fitness effects. Driver acquisition during the Darwinian precancerous stage may be modulated en route to neutral evolution by the combination of two factors: spatial constraints and limited cellular mixing. These two factors occur naturally in ductal carcinomas, where the branching topology of the ductal network dictates spatial constraints and mixing rates.},
	pages = {2060},
	number = {1},
	journaltitle = {Nature Communications},
	shortjournal = {Nat Commun},
	author = {West, Jeffrey and Schenck, Ryan O. and Gatenbee, Chandler and Robertson-Tessi, Mark and Anderson, Alexander R. A.},
	urldate = {2023-07-28},
	date = {2021-04-06},
	langid = {english},
	note = {Number: 1
Publisher: Nature Publishing Group},
	keywords = {Tumour heterogeneity, Cancer},
	file = {Full Text PDF:/Users/vesheljinn/Zotero/storage/6P7JV983/West et al. - 2021 - Normal tissue architecture determines the evolutio.pdf:application/pdf},
}

@article{traulsen_fastest_2007,
	title = {The fastest evolutionary trajectory},
	volume = {249},
	issn = {0022-5193},
	url = {https://www.ncbi.nlm.nih.gov/pmc/articles/PMC2384164/},
	doi = {10.1016/j.jtbi.2007.08.012},
	abstract = {Given two mutants, A and B, separated by n mutational steps, what is the evolutionary trajectory which allows a homogeneous population of A to reach B in the shortest time? We show that the optimum evolutionary trajectory (fitness landscape) has the property that the relative fitness increase between any two consecutive steps is constant. Hence, the optimum fitness landscape between A and B is given by an exponential function. Our result is precise for small mutation rates and excluding back mutations. We discuss deviations for large mutation rates and including back mutations. For very large mutation rates, the optimum fitness landscape is flat and has a single peak at type B.},
	pages = {617--623},
